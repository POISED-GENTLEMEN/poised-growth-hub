{
  "rewrites": [
    {
      "source": "/admin/config.yml",
      "destination": "/admin/config.yml"
    },
    {
      "source": "/admin/config.yml/",
      "destination": "/admin/config.yml"
    },
    {
      "source": "/admin/config.yml:match*",
      "destination": "/admin/config.yml"
    }
  ],
  "routes": [
    {
      "src": "^/admin/?$",
      "dest": "/admin/index.html"
    },
    {
      "src": "^/admin/(.*)$",
      "dest": "/admin/index.html"
    }
  ]
}
